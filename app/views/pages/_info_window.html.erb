<h2><%= venue.name %></h2>
<% if venue.photos.attached? %>
  <% venue.photos.each do |photo| %>
    <%= cl_image_tag photo.key, crop: :fill %>
  <% end %>
<% else %>
  <%= cl_image_tag "https://res.cloudinary.com/wvala/image/upload/v1653657110/nxr4ymq5mgmmfmwflkqv.jpg" %>
<% end %>
<p>Location: <%= venue.address %></p>
